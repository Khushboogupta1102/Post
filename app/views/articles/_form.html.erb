<%= form_with model: article, local: true, multipart: true do |form| %>

    <div>
      <%= form.label :title %><br>
      <%= form.text_field :title %>
    </div>

     <div>
      <%= form.label :image, "Image" %><br>
      <%= form.file_field :image %>

    </div>
  
    <div>
      <%= form.label :body %><br>
      <%= form.text_area :body %><br>
    </div>
  
    <div>
      <%= form.submit %>
    </div>
  <% end %>
  